function clamp(x, min, max)
	if(x < min) then x = min
	else if(x > max) then x = max end end
	return x 
end

function lerp(a,b,t) return a+(b-a)*t end
function dist(x1,y1, x2,y2) return sqrt((x2-x1)^2+(y2-y1)^2) end

