		newshot.spritex = 16
		newshot.spritey = 8
		newshot.spritew = 9
		newshot.spriteh = 9
		newshot.flipx = true
		newshot.flipy = false
		cameraoffsetx -= 4 / sqrt(2)
		cameraoffsety -= 4 / sqrt(2)
		player.dx += .5 / sqrt(2) 
		player.dy += .5 / sqrt(2)
	end

	if(shotdir == "southeast") then
		newshot.x = player.x + 1
		newshot.y = player.y + 4
		newshot.spritex = 16
		newshot.spritey = 8
		newshot.spritew = 9
		newshot.spriteh = 9
		newshot.flipx = false
		newshot.flipy = true
		cameraoffsetx += 4 / sqrt(2)
		cameraoffsety += 4 / sqrt(2)
		player.dx -= .5 / sqrt(2) 
		player.dy -= .5 / sqrt(2)
	end

	if(shotdir == "southwest") then
		newshot.x = player.x - 12
		newshot.y = player.y +4
		newshot.spritex = 16
		newshot.spritey = 8
		newshot.spritew = 9
		newshot.spriteh = 9
