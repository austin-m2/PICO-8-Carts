pico-8 cartridge // http://www.pico-8.com
version 5
__lua__

--initialize controls
btn_left = false
btn_right = false
btn_up = false
btn_down = false
btn_z = false
btn_x = false


function _init()
	px = 20
	py = 64
	pdx = 0
	pdy = 0
	pstate = 0
	pspr = 0
	pdir = 0
	pat = 0
	movespeed = 2
	accel = 1
	
	xdir=0
	ydir=0
	

end

function _update()
	check_controls()
	
	--px = (px+128)/128 --screen wrappin'
	pat+=1  --increment state timer
	
	--idle state
	if pstate==0 then
		pspr = 0
		--if (bxor(btn_left,btn_right) or bxor(btn_down,btn_up)) change_state(1) end
		if(btn_left or btn_right or btn_down or btn_up) change_state(1) end
	end
	
	--walk state
	if pstate==1 then
		xdir = 0
		ydir = 0
		
		if(btn_left and btn_right) then xdir = 0
		else 
				if(btn_left) then xdir = -1 end
				if(btn_right) then xdir = 1 end
		end
		
		if(btn_up and btn_down) then ydir = 0
		else 
			if(btn_up) then ydir = -1 end
			if(btn_down) then ydir = 1 end
		end
		
		--if(bxor((xdir != 0),(ydir != 0))) then     
		if((xdir != 0) or (ydir != 0)) then
			if not ((xdir != 0) and (ydir !=0)) then
				pdx = approach(pdx,movespeed*xdir,accel)
				pdy = approach(pdy,movespeed*ydir,accel)
			end
		else 
			if((xdir != 0) and (ydir !=0)) then
				pdx = approach(pdx,movespeed*xdir/sqrt(2),accel)
				pdy = approach(pdy,movespeed*xdir/sqrt(2),accel)
			else
				pdx = approach(pdx,0,accel)
				pdy = approach(pdy,0,accel)
			end
		end
		
		if(pdx==0 and pdy==0) then state_switch(0) end
		
		px+=pdx
		py+=pdy
		
		
	end
end


function _draw()
	cls()
	spr(pspr,px,py,1,1,pdir==-1)
	--print(bxor(btn_left,btn_right),10,10)
	print(((xdir != 0) or (ydir !=0)) and (not((xdir != 0) and (ydir !=0))),10,10)
	print(xdir,10,20)
end


function change_state(s)
	pstate = s
	pat = 0
end

function check_controls()
	btn_left = btn(0)
	btn_right = btn(1)
	btn_up = btn(2)
	btn_down = btn(3)
	btn_z = btn(4)
	btn_x = btn(5)
end


function approach(arg0,arg1,arg2)
	if(arg0 < arg1) then
		return min(arg0 + arg2, arg1)
	else
		return max(arg0 - arg2, arg1)
	end
end

