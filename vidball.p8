__lua__

-- utility --------------------
function dist(x1,y1,x2,y2)
 return sqrt((x2-x1)^2+(y2-y1)^2)
end
-------------------------------
music(0)

p_len = 5 --originally 7
p_wid = 2.5 --originally 3
p_spd = 1
p_acl = 0.150
p_drg = 0.025
p_rad = 2.5

function p_make(x, y, r)
	return {
		x=x, --position
		y=y,
		vx=0, --velocity
		vy=0,
		rot=r, --rotation
		chrg=0, --charge
		col=12, --color
		inv=0,
		p=0 --controller index
	}
end

function p_update(p)
	-- invuln clock
	p.inv=max(0,p.inv-1)
	
	-- shooting
	if btn(4,p.p) then
		p.chrg=min(1,p.chrg+0.05)
	end
	if not btn(4,p.p) and p.chrg > 0 then
		local s = s_recycle(
			p.x+cos(p.rot)*p_len*0.6,
			p.y+sin(p.rot)*p_len*0.6,
			p.chrg,
			p.rot,
			p.p
		)
		s.vx+=p.vx--relative motion
		s.vy+=p.vy
		--reset chrg
		p.chrg = 0
	end
	
	-- turning
	if btn(0,p.p) then
		p.rot+=0.025
	end
	if btn(1,p.p) then
		p.rot-=0.025
	end
	
	--acceleration
	if btn(2,p.p) then
		p.vx+=cos(p.rot)*p_acl
		p.vy+=sin(p.rot)*p_acl
		--velocity capping
		if abs(p.vx)+abs(p.vy) > 4 then
			local d=atan2(p.vx,p.vy)
			p.vx=cos(d)
			p.vy=sin(d)
		end	
	end
	
	-- drag
	local vel=abs(p.vx)+abs(p.vy)
	local sx=sgn(p.vx)
	local sy=sgn(p.vy)
	p.vx-=p_drg*p.vx*vel
	p.vy-=p_drg*p.vy*vel
	if(sx != sgn(p.vx))p.vx=0
	if(sy != sgn(p.vy))p.vy=0
	
	-- movement
	p.x+=p_spd*p.vx
	p.y+=p_spd*p.vy
	
	--top/bottom wall collision
	if(p.y<=(17+p_rad)) then
		while(p.y<=(17+p_rad)) do
			p.y+=1
		end
		p.vy=-(p.vy*.75)
	end
	
	if(p.y>=(110-p_rad)) then
		while (p.y>=(110-p_rad)) do
			p.y-=1
		end
		p.vy=-(p.vy*.75)
	end
	
	
end

function _init()
	b_init()
	p1 = p_make(32,64,0)
	s_init()
end


function _update()
	--rotateplayer(p1, -0.01)
	
	s_update()
	p_update(p1)
	b_update()
	
	
	
	
	--p1.topy=sin(angle)*(px-p1.x)+cos(angle) * (py-p1.y) + p1.y
end

function _draw()
	rectfill(0,0,127,127,15) --court
	circ(64,64,10,7) --outer circle
	circ(64,64,3,7) --inner circle
	line(45,18,45,126,7) --mid left line
	line(82,18,82,126,7) --mid right line
	
	line(10,18,10,126,7) --left line
	rectfill(0,18,9,109,12) --left goal
	line(117,18,117,126,7) --right line
	rectfill(118,18,127,127,14) --right goal
	
	s_draw()
	p_draw(p1)
	b_draw()

	rectfill(0,0,127,16,2) --top
	rectfill(0,111,127,127,2) --bottom
	line(0,17,127,17,0) --top line
	line(0,110,127,110) --bottom line
	spr(0,2,112,16,2)
	
	-- cls()
	-- line(p1.topx,p1.topy,p1.rightx,p1.righty)
	-- line(p1.topx,p1.topy,p1.leftx,p1.lefty)
 -- line(p1.botx,p1.boty,p1.rightx,p1.righty)
 -- line(p1.botx,p1.boty,p1.leftx,p1.lefty)
end

function p_draw(p)
 -- how much the ship extends in front
 local lenx = cos(p.rot)*p_len
 local leny = sin(p.rot)*p_len
 -- how much the ship extends to the side
 local sidex = cos(p.rot+0.25)*p_wid
 local sidey = sin(p.rot+0.25)*p_wid
 -- point at front of ship
 local pfx=p.x+lenx*0.6
 local pfy=p.y+leny*0.6
 -- point at mid-back of ship
 local pmx=p.x-lenx*0.1
 local pmy=p.y-leny*0.1
 -- point at left of ship
 local plx=p.x-lenx*0.4+sidex
 local ply=p.y-leny*0.4+sidey
 -- point at right of ship
 local prx=p.x-lenx*0.4-sidex
 local pry=p.y-leny*0.4-sidey
 -- thats a lot of math! easy part now!
 if p.inv % 6 < 3 then

  --let's try to draw an outline
  if(pget(p.x,p.y)==p.col) then
    --circfill(p.x,p.y,1,7)
	line(pfx,pfy,plx,ply,1)
	line(pfx,pfy,prx,pry,1)
	line(plx,ply,pmx,pmy,1)
	line(pmx,pmy,prx,pry,1)
  else
    --circfill(p.x,p.y,1,7)
	line(pfx,pfy,plx,ply,p.col)
	line(pfx,pfy,prx,pry,p.col)
	--line(plx,ply,prx,pry,p.col)
	line(plx,ply,pmx,pmy,p.col)
	line(pmx,pmy,prx,pry,p.col)
  end
 end
 -- charge graphic
 if p.chrg > 0 then
	circfill(pfx,pfy,p.chrg*3,10)
 end
end

