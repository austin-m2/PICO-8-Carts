s_spd_min = 2
s_spd_chg = 1.5

function s_new(t,x,y,s,d,p)
 t = t or {}
 t.x=x
 t.y=y
 t.s=s--size(0 to 1)
 t.sz=1+s*3--pixel size
 t.d=d--dir
 t.dst=50--dist to travel
 t.on=true
 t.p=p--player
 t.spd=s_spd_min+s_spd_chg*s
 t.vx=cos(d)*t.spd
 t.vy=sin(d)*t.spd
 return t
end

function s_recycle(...)
 shots_on+=1
 for s in all(shots) do
  if s.on==false then
   s = s_new(s,...)
   return s
  end
 end
 local s = s_new(nil,...)
 shots[#shots+1]=s
 return s
end

